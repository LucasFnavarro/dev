@page
@model RazorPages.Pages.Index

<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <title>Razor Pages</title>
</head>
<body>
<div>
    
    <table>
        <thead>
        <tr>
            <td>ID</td>
            <td>Title</td>
            <td>Price</td>
        </tr>
        </thead>
        <tbody>
        @foreach (var category in Model.Categories)
        {
            <tr>
                <td>@category.Id</td>
                <td>@category.Title</td>
                <td>@category.Price</td>
            </tr>
        }
        </tbody>
    </table>

</div>
</body>
</html>